    "typescript": "^3.4.4"
  },
  "jest": {
    "preset": "react-native"
  },
  "name": "mobile",
  "precommit": "lint",
  "private": true,
  "scripts": {
    "android": "ANDROID_HOME=${HOME}/Library/Android/sdk react-native run-android --variant debug",
    "android:alpha": "ANDROID_HOME=${HOME}/Library/Android/sdk bundle exec fastlane android alpha",
    "android:beta": "ANDROID_HOME=${HOME}/Library/Android/sdk bundle exec fastlane android beta",
    "android:build": "ANDROID_HOME=${HOME}/Library/Android/sdk bundle exec fastlane android build",
    "android:codepush": "appcenter codepush release-react -a techkids/mobile-template-android",
    "android:codepush:prod": "appcenter codepush promote -a techkids/mobile-template-android -s Staging -d Production ",
    "android:emulator": "yarn emulator & yarn android",
    "android:production": "ANDROID_HOME=${HOME}/Library/Android/sdk bundle exec fastlane android production",
    "android:release": "ANDROID_HOME=${HOME}/Library/Android/sdk cd android && ./gradlew assembleRelease --stacktrace && cd ..",
    "android:release-test": "ANDROID_HOME=${HOME}/Library/Android/sdk react-native run-android --variant release",
    "emulator": "${HOME}/Library/Android/sdk/tools/emulator -avd android-emulator",
    "ios": "react-native run-ios --simulator=\"iPhone X\"",
    "ios6": "react-native run-ios --simulator=\"iPhone 6\"",
    "ios:testflight": "bundle exec fastlane ios uploadToTestflight",
    "ios:build": "bundle exec fastlane ios build",
    "ios:codepush": "code-push release-react techkids/mobile-template-android",
    "ios:codepush:prod": "appcenter codepush promote -a techkids/mobile-template-ios -s Staging -d Production ",
    "ios:production": "bundle exec fastlane ios production",
    "job": "ts-node job.ts",
    "update-version": "ts-node job.ts update-version",
    "lint": "tsc -p . && tslint -p .",
    "start": "yarn lint && node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "updateIcon": "yo rn-toolbox:assets --icon ./src/assets/images/appIcon.png"
  },
  "version": "0.1.0"
}