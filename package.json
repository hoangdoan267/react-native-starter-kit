    "@types/react-native": "^0.57.46",
    "@types/react-native-fbsdk": "^0.6.3",
    "@types/react-native-vector-icons": "^6.4.0",
    "@types/react-redux": "^7.0.1",
    "@types/react-test-renderer": "^16.8.1",
